---
  env:
    browser: true
    node: true
    jasmine: true
    jquery: true
    es6: true
  parserOptions:
    ecmaFeatures:
      jsx: true
  parser: "babel-eslint"
  plugins:
    - "react"
  globals:
    ACE: true
    angular: true
    _: true
    inject: true
    module: true
  rules:
    comma-dangle:
      - 2
      - "never"
    no-cond-assign:
      - 2
      - "always"
    no-console: 1
    no-debugger: 1
    no-constant-condition: 1
    no-dupe-args: 2
    no-dupe-keys: 2
    no-duplicate-case: 2
    no-empty: 2
    no-ex-assign: 2
    no-extra-boolean-cast: 2
    no-extra-parens:
      - 1
      - "functions"
    no-extra-semi: 2
    no-func-assign: 2
    no-inner-declarations: 2
    no-invalid-regexp: 2
    no-irregular-whitespace: 2
    no-negated-in-lhs: 2
    no-obj-calls: 2
    no-regex-spaces: 2
    quote-props:
      - 2
      - "as-needed"
      -
        keywords: true
        unnecessary: false
    no-sparse-arrays: 2
    no-unreachable: 2
    use-isnan: 2
    valid-jsdoc: 1
    valid-typeof: 2
    no-unexpected-multiline: 2
    accessor-pairs: 2
    block-scoped-var: 2
    complexity:
      - 0
      - 10
    consistent-return: 2
    curly:
      - 2
      - "multi-line"
    default-case: 2
    dot-notation:
      - 2
      -
        allowPattern: "^[A-Z]"
    dot-location:
      - 2
      - "property"
    eqeqeq:
      - 2
      - "allow-null"
    guard-for-in: 2
    no-alert: 1
    no-caller: 2
    no-else-return: 2
    no-eval: 2
    no-extend-native: 2
    no-extra-bind: 2
    no-fallthrough: 2
    no-floating-decimal: 2
    no-implied-eval: 2
    no-iterator: 2
    no-labels: 1
    no-lone-blocks: 2
    no-loop-func: 2
    no-multi-spaces: 2
    no-multi-str: 2
    no-native-reassign: 2
    no-new-func: 2
    no-new-wrappers: 2
    no-new: 2
    no-octal-escape: 2
    no-octal: 2
    no-proto: 2
    no-redeclare: 2
    no-return-assign:
      - 2
      - "except-parens"
    no-script-url: 2
    no-self-compare: 2
    no-sequences: 2
    no-throw-literal: 2
    no-unused-expressions: 2
    no-with: 2
    radix: 2
    vars-on-top: 2
    wrap-iife:
      - 2
      - "any"
    yoda: 2
    strict:
      - 2
      - "global"
    no-catch-shadow: 2
    no-delete-var: 2
    no-label-var: 2
    no-shadow-restricted-names: 2
    no-shadow: 2
    no-undef-init: 2
    no-undef: 2
    no-unused-vars: 2
    react/jsx-uses-vars: 2
    react/jsx-uses-react: 2
    no-use-before-define:
      - 2
      - "nofunc"
    array-bracket-spacing: 2
    brace-style:
      - 2
      - "1tbs"
      -
        allowSingleLine: true
    camelcase:
      - 2
      -
        properties: "never"
    comma-spacing:
      - 2
      -
        before: false
        after: true
    comma-style:
      - 2
      - "last"
    eol-last: 2
    key-spacing: 0
    new-cap: 2
    new-parens: 2
    no-array-constructor: 2
    no-mixed-spaces-and-tabs: 2
    no-nested-ternary: 2
    no-new-object: 2
    no-spaced-func: 2
    no-trailing-spaces: 2
    no-underscore-dangle: 0
    no-unneeded-ternary: 2
    quotes:
      - 2
      - "single"
    semi-spacing:
      - 2
      -
        before: false
        after: true
    semi:
      - 2
      - "always"
    keyword-spacing: 2
